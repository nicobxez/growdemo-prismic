{
  "id": "product",
  "label": "Product",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": "URL slug for the product"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Title of the product",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "price": {
        "type": "Number",
        "config": {
          "label": "Price",
          "placeholder": "Price for the product"
        }
      },
      "rating": {
        "type": "Number",
        "config": {
          "label": "Rating",
          "placeholder": "How popular is your product"
        }
      },
      "featuredImage": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {
            "width": 2000,
            "height": 1500
          },
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "contact_form": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}