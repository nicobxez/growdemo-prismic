{
  "id": "settings",
  "label": "Settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Nombre de tu app",
          "placeholder": "Nombre de tu app (ej: Prismic)",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Descripción ",
          "placeholder": "Breve descripción de tu app",
          "allowTargetBlank": false,
          "single": "paragraph"
        }
      },
      "banner": {
        "type": "Image",
        "config": {
          "label": "Foto de portada",
          "constraint": {
            "width": 2000,
            "height": 2000
          },
          "thumbnails": []
        }
      },
      "formTitle": {
        "type": "StructuredText",
        "config": {
          "label": "Titulo de formulario",
          "placeholder": "Texto para título de suscripción",
          "allowTargetBlank": true,
          "multi": "paragraph,heading1,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "formDescription": {
        "type": "StructuredText",
        "config": {
          "label": "Descripción de formulario",
          "placeholder": "Texto acerca de suscripción",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      }
    }
  }
}